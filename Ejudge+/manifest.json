{
  // Required
  "manifest_version": 2,
  "name": "Ejudge+",
  "version": "1.1",

  // Recommended
  "icons": {
    "16": "img/main16.png",
    "32": "img/main32.png",
    "48": "img/main48.png",
    "128":"img/main128.png"
  },

  "description": "Ejudge tools",
  // "icons": {...},

  // Pick one (or none)
  "page_action": {
    "default_popup": "html/popup.html",
    "default_title": "Ejudge+"
  },

  //   "page_action": {...},
  "background": {
    "scripts": ["js/background.js"]
  },

  "permissions": ["tabs", "storage"],

  "content_scripts": [
    {
      "matches": ["http://caos.ejudge.ru/*", "http://caos.ejudge.ru/*"],
      "css": ["css/style.css"],
      "js": ["js/userscript.js"]
    }
  ],

  "web_accessible_resources": [
    "img/logo.png"
  ],

  "options_page": "html/popup.html"

